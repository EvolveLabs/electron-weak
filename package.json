  "name": "electron-weak",
  "description": "This is a fork of the node-weak project, adding electron builds and binaries.",
  "keywords": [
    "weak",
    "reference",
    "js",
    "javascript",
    "object",
    "function",
    "callback"
  ],
  "version": "0.4.6",
  "repository": {
    "type": "git",
    "url": "git://github.com/evolvelabs/electron-weak.git"
  },
  "main": "lib/weak.js",
  "scripts": {
    "test": "mocha -gc --reporter spec",
    "install": "bash ./node_modules/.bin/elinst"
  },
  "engines": {
    "node": ">=2.3.1"
